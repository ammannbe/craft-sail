{
    "name": "suhype/craft-sail",
    "description": "Docker files for running a basic Craft application.",
    "keywords": ["craft", "craftcms", "docker"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ammannbe/craft-sail/issues",
        "source": "https://github.com/ammannbe/craft-sail"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Benjamin Ammann",
            "homepage": "https://github.com/ammannbe"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "bin": [
        "bin/sail"
    ],
    "autoload": {
        "psr-4": {
            "suhype\\sail\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpstan/phpstan": "^1.10"
    }
}
